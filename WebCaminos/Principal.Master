<body style="height: 100%">
    <form id="form1" runat="server" style="height: 100%" autocomplete="off" method="post">
        <asp:ScriptManager ID="scpGeneral" runat="server"></asp:ScriptManager>
        <div id="cabecera">
            <%--<div id="logo_fassil_escritorio" onclick=""></div>
            <div id="logo_fassil_net">
                <div id="opciones_header">
                    <asp:LinkButton runat="server" ID="contact" OnClick="contact_Click" OnClientClick="contact()">Contáctenos&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;</asp:LinkButton>

                    <asp:LinkButton runat="server" ID="salir" OnClick="salir_Click">Salir</asp:LinkButton>
                </div>
            </div>--%>
            <div id="opciones_header">
                <asp:LinkButton runat="server" Style="padding-right: 15px;" ID="salir" OnClick="salir_Click">Salir</asp:LinkButton>
            </div>
            <div id="encabezado_smart">
