    <script type="text/javascript">
        function scrolltop() {
            $("html, body").animate({ scrollTop: 0 }, 600);
        }

        function redireccionar() {
            $.blockUI({
                message: $('#Block_div'),
                css:
                {
                    padding: 0,
                    margin: 0,
                    textAlign: 'center',
                    fontSize: 12,
                    cursor: 'auto',
                    border: 'none',
                    background: 'none'
                }
            });
            window.location = '<%= ResolveUrl("~/AutorizacionesPendientes.aspx") %>';
        }

        function redireccionar_contactenos() {
            $.blockUI({
                message: $('#Block_div'),
                css:
                {
                    padding: 0,
                    margin: 0,
                    textAlign: 'center',
                    fontSize: 12,
                    cursor: 'auto',
                    border: 'none',
                    background: 'none'
                }
            });
            window.location = '<%= ResolveUrl("~/Contactenos.aspx") %>';
        }

        function contact() {
            $.blockUI({
                message: $('#Block_div'),
                css:
                {
                    padding: 0,
                    margin: 0,
                    textAlign: 'center',
                    fontSize: 12,
                    cursor: 'auto',
                    border: 'none',
                    background: 'none'
                }
            });
        }

        function BlockMaster(control) {
            var hr = $(control).attr("href");
            if (hr != "#") {
                $.blockUI({
                    message: $('#Block_div'),
                    css:
                    {
                        padding: 0,
                        margin: 0,
                        textAlign: 'center',
                        fontSize: 12,
                        cursor: 'auto',
                        border: 'none',
                        background: 'none'
                    }
                });
            }
        }

        function scrolltop() {
            $("html, body").animate({ scrollTop: 0 }, "slow");
            return false;
        }

        function loadCombos() {
            //var nro = '%=ConfigurationManager.AppSettings["MaximoCombo"].ToString() %>'
            var nro = 20;
            $(".custom-dropdown select").chosen({ disable_search_threshold: nro, placeholder_text_single: "Seleccionar" });
        }

        //function loadPassword() {
        //    $('input[type=password]').disableAutocomplete();
        //}
    </script>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
