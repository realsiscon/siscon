                    <h2 id="nro_notificaciones_smart" runat="server"></h2>
                    <img src="Slices/icon_zona_segura_smart.png" class="iconos_cabecera_smart" />
                    <img src="Slices/Contactanos.png" class="iconos_cabecera_smart" style="padding-top: 5px" onclick="redireccionar_contactenos()" />
                    <asp:LinkButton runat="server" ID="salir_smart" OnClick="salir_Click">Salir</asp:LinkButton>
                </div>
            </div>

        </div>
        <div id="contenedor">

            <div id="menu_tablet">
                <img id="ic_menu_tablet" src="Slices/icon_menu_tablet.png" onclick="mostrar_tablet_menu()" />
                <img id="icNotTablet" src="Slices/icon_notificaciones_tablet.png" onclick="redireccionar()" runat="server" visible="false" />
                <h2 id="nro_notificaciones_tablet" runat="server"></h2>
                <h2 class="texto_bienvenida">Bienvenido:&nbsp;</h2>
                <h2 id="tCliente" runat="server"></h2>
                <br />
                <h2 class="texto_bienvenida">Representante:&nbsp;</h2>
                <h2 id="tRepresentante" runat="server"></h2>
            </div>

            <div id="Block_div" style="width: 100%; display: none;">
                <img src="Imagenes/loading.gif" title="Click here to unblock" />
            </div>

            <div id="segunda_cabecera_1">
                <div id="NombresUsuarios">
                    <h2 class="texto_bienvenida">Bienvenido:&nbsp;</h2>
                    <h2 id="hCliente" runat="server"></h2>

                    <h2 class="texto_bienvenida">Correo:&nbsp;</h2>
                    <h2 id="hRepresentante" runat="server"></h2>
                </div>
                <h2 id="fecha_hora">Fecha: <strong>
                    <asp:Label ID="lblFecha" runat="server" CssClass="lblFecha"></asp:Label>
                    |</strong>
                    Hora: <strong>
